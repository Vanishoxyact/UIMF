type check from tostring
function is_pendingbattle(obj)
	if string.sub(tostring(obj), 1, 31) == "PENDING_BATTLE_SCRIPT_INTERFACE" then
		return true;
	end;
	
	return false;
end;
setmetatable(self, {
    __index = Button,
    __tostring = function() return "BUTTON: "..name end
})

